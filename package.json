{
  "name": "le-challenge-redis",
  "version": "1.0.0",
  "description": "A redis-based strategy for node-letsencrypt for setting, retrieving, and clearing ACME challenges issued by the ACME server",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "redis": "^2.8.0",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^10.11.2",
    "@types/redis": "^2.6.0",
    "jest": "^22.1.4",
    "ts-jest": "^23.10.1",
    "typescript": "^2.5.2"
  },
  "keywords": [
    "acme",
    "challenge",
    "cluster",
    "ephemeral",
    "http",
    "http-01",
    "le",
    "le-challenge",
    "le-challenge-",
    "le-challenge-redis",
    "le-challenge-memory",
    "le-challenge-database",
    "letsencrypt",
    "greenlock",
    "node-greenlock",
    "redis"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com> (https://caseyWebb.xyz/)",
  "license": "WTFPL",
  "homepage": "https://github.com/caseyWebb/le-challenge-redis#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caseyWebb/le-challenge-redis.git"
  },
  "bugs": {
    "url": "https://github.com/caseyWebb/le-challenge-redis/issues"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts"
    ],
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
